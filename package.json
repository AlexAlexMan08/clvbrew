{
	"name": "clvbrew",
	"displayName": "Clvbrew",
	"description": "obfuscate with clvbrew",
	"publisher": "Burkino",
	"repository": {"directory": "https://github.com/Burkino/clvbrew"},
	"icon": "gamertime.png",
	"version": "0.0.5",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"configuration": {
			"title": "Clvbrew",
			"properties": {
				"clvbrew.OutputType": {
					"type": "string",
					"default": "Create new file",
					"description": "Where to output obfuscated script",
					"enum": [
						"Replace current file",
						"Create new file"
					],
					"enumDescriptions": [
						"Replaces entire document with obfuscated script",
						"Creates new document with obfuscated script"
					]
				},
				"clvbrew.ApiKey": {
					"type": "string",
					"default": "",
					"description": "API key used for Clvbrew"
				},
				"clvbrew.encAllStrings": {
					"type": "boolean",
					"default": false,
					"description": "Encrypt all strings?"
				},
				"clvbrew.encImportantStrings": {
					"type": "boolean",
					"default": false,
					"description": "Encrypt important strings?"
				},
				"clvbrew.noControlFlow": {
					"type": "boolean",
					"default": false,
					"description": "Obfuscate Control Flow?"
				},
				"clvbrew.debugInfo": {
					"type": "boolean",
					"default": false,
					"description": "Keep debug info?"
				},
				"clvbrew.noCompressBS": {
					"type": "boolean",
					"default": false,
					"description": "Compress bytecode?"
				}
			}
		},
		"commands": [
			{
				"command": "clvbrew.obfuscate",
				"title": "Clvbrew: Obfuscate Script"
			},
			{
				"command": "clvbrew.changelog",
				"title": "Clvbrew: Obfuscator Changelog"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.45.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"node-fetch": "^2.6.0"
	}
}
